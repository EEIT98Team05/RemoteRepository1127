<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="article" class="org.springframework.web.servlet.view.RedirectView">
		<property name="contextRelative" value="true"></property>
		<property name="url" value="/admin/production/article.jsp"></property>
	</bean>

	<bean id="matching" class="org.springframework.web.servlet.view.RedirectView">
		<property name="contextRelative" value="true"></property>
		<property name="url" value="/admin/production/partner.jsp"></property>
	</bean>
	
	<bean id="index" class="org.springframework.web.servlet.view.RedirectView">
		<property name="contextRelative" value="true"></property>
		<property name="url" value="/index.jsp"></property>
	</bean>
</beans>
